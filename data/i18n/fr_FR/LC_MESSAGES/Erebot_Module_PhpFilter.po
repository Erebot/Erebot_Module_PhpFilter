msgid   "Usage: <b><var name=\"cmd\"/> &lt;filter&gt; &lt;text&gt;</b>. "
        "Available filters: <for from=\"filters\" item=\"filter\"><var name="
        "\"filter\"/></for>."
msgstr  "Usage: <b><var name=\"cmd\"/> &lt;filter&gt; &lt;text&gt;</b>. "
        "Filtres disponibles : <for from=\"filters\" item=\"filter\"><var name="
        "\"filter\"/></for>."
#
msgid   "No such filter \"<var name=\"filter\"/>\" or filter blocked."
msgstr  "Filtre introuvable \"<var name=\"filter\"/>\" ou filtre bloqué."
#
msgid   "Could not register Filter trigger"
msgstr  "Impossible d'enregistrer le déclencheur des Filtres"
#
msgid   "\n"
        "Provides the <b><var name=\"trigger\"/></b> command which transforms "
        "the given\n"
        "input using some PHP filter.\n"
msgstr  "\n"
        "Fournit la commande <b><var name=\"trigger\"/></b> qui transforme une "
        "entrée en utilisant un filtre de PHP.\n"
#
msgid   "\n"
        "<b>Usage:</b> !<var name=\"trigger\"/> &lt;<u>filter</u>&gt; &lt;"
        "<u>input</u>&gt;.\n"
        "Transforms the given &lt;<u>input</u>&gt; using the given &lt;"
        "<u>filter</u>&gt;.\n"
        "The following filters are available: <for from=\"filters\" item="
        "\"filter\">\n"
        "<b><var name=\"filter\"/></b></for>.\n"
msgstr  "\n"
        "<b>Usage :</b> !<var name=\"trigger\"/> &lt;<u>filtre</u>&gt; &lt;"
        "<u>entrée</u>&gt;.\n"
        "Transforme l'&lt;<u>entrée</u>&gt; en utilisant le &lt;<u>filtre</"
        "u>&gt; spécifié.\n"
        "Les filtres suivants sont disponibles : <for from=\"filters\" item="
        "\"filter\">\n"
        "<b><var name=\"filter\"/></b></for>.\n"
#
